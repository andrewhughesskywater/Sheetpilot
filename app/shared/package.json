{
  "name": "@sheetpilot/shared",
  "version": "1.6.0",
  "private": true,
  "type": "module",
  "description": "Shared TypeScript contracts and utilities for Sheetpilot",
  "main": "./index.ts",
  "types": "./index.ts",
  "sideEffects": [
    "./logger.ts",
    "./logger-config.ts",
    "./business-config.window.ts"
  ],
  "exports": {
    ".": "./index.ts",
    "./*": "./*.ts"
  },
  "files": [
    "**/*.ts"
  ],
  "peerDependencies": {
    "electron-log": "^5.4.3"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0 --cache --ignore-pattern \"tests/**\"",
    "lint:fix": "eslint . --max-warnings 0 --cache --ignore-pattern \"tests/**\" --fix",
    "typecheck": "tsc -p ../../tsconfig.typecheck.json --noEmit"
  }
}
